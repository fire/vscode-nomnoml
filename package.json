{
  "name": "vscode-nomnoml",
  "displayName": "nomnoml",
  "description": "A tool for rendering UML diagrams based on the nomnoml library.",
  "version": "0.4.0",
  "publisher": "fire",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/fire/vscode-nomnoml.git"
  },
  "bugs": {
    "url": "https://github.com/fire/vscode-nomnoml/issues"
  },
  "engines": {
    "vscode": "^1.0.0"
  },
  "keywords": [
    "nomnoml",
    "uml",
    "diagrams"
  ],
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.nomnoml"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "languages": [
      {
        "id": "nomnoml",
        "extensions": [
          ".nomnoml"
        ]
      }
    ],
    "commands": [
      {
        "command": "extension.nomnoml",
        "title": "View: nomnoml"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "node ./node_modules/vscode/bin/compile",
    "compile": "node ./node_modules/vscode/bin/compile -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "dependencies": {
    "nomnoml": "1.4.0"
  },
  "devDependencies": {
    "typescript": "^4.5.4",
    "vscode": "^1.1.37"
  }
}
